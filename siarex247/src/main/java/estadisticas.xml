<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 6.0//EN"
       "https://struts.apache.org/dtds/struts-6.0.dtd">

<struts>


	  <package name="repVerificacion" namespace="/estadisticas/repVerificacion" extends="struts-default" strict-method-invocation="false">
  		
	   <result-types>
	   		<result-type name="json" class="org.apache.struts2.json.JSONResult"/>
	   	</result-types>
	   	<interceptors>	
			<interceptor name="setHibernateSession" class="org.apache.struts2.json.JSONInterceptor"></interceptor>
	         <interceptor-stack name="newStack">
	         	<interceptor-ref name="setHibernateSession"/>
				<interceptor-ref name="defaultStack" />
	         </interceptor-stack>
		  </interceptors>	
		  
        <action name="*" class="com.siarex247.estadisticas.RepVerificacion.RepVerificacionAction" method="{1}">
            <interceptor-ref name="newStack"/>
            <result type="json" />
        </action>
        
    </package>
    
    
    <package name="repCartaPorte" namespace="/estadisticas/repCartaPorte" extends="struts-default" strict-method-invocation="false">
  		
	   <result-types>
	   		<result-type name="json" class="org.apache.struts2.json.JSONResult"/>
	   	</result-types>
	   	<interceptors>	
			<interceptor name="setHibernateSession" class="org.apache.struts2.json.JSONInterceptor"></interceptor>
	         <interceptor-stack name="newStack">
	         	<interceptor-ref name="setHibernateSession"/>
				<interceptor-ref name="defaultStack" />
	         </interceptor-stack>
		  </interceptors>	
		  
        <action name="*" class="com.siarex247.estadisticas.RepValidacion.RepValidacionAction" method="{1}">
            <interceptor-ref name="newStack"/>
            <result type="json" />
        </action>
        
    </package>
          
    </struts>
